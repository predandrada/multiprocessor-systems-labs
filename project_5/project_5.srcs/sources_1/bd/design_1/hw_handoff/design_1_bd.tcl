# Procedure to create entire design; Provide argument to make
# procedure reusable. If parentCell is "", will use root.
proc create_root_design { parentCell } {

  variable script_folder
  variable design_name

  if { $parentCell eq "" } {
     set parentCell [get_bd_cells /]
  }

  # Get object for parentCell
  set parentObj [get_bd_cells $parentCell]
  if { $parentObj == "" } {
     catch {common::send_msg_id "BD_TCL-100" "ERROR" "Unable to find parent cell <$parentCell>!"}
     return
  }

  # Make sure parentObj is hier blk
  set parentType [get_property TYPE $parentObj]
  if { $parentType ne "hier" } {
     catch {common::send_msg_id "BD_TCL-101" "ERROR" "Parent <$parentObj> has TYPE = <$parentType>. Expected to be <hier>."}
     return
  }

  # Save current instance; Restore later
  set oldCurInst [current_bd_instance .]

  # Set parent object as current
  current_bd_instance $parentObj


  # Create interface ports

  # Create ports
  set oe_0 [ create_bd_port -dir I oe_0 ]

  # Create instance: PIC16F873_0, and set properties
  set block_name PIC16F873
  set block_cell_name PIC16F873_0
  if { [catch {set PIC16F873_0 [create_bd_cell -type module -reference $block_name $block_cell_name] } errmsg] } {
     catch {common::send_msg_id "BD_TCL-105" "ERROR" "Unable to add referenced block <$block_name>. Please add the files for ${block_name}'s definition into the project."}
     return 1
   } elseif { $PIC16F873_0 eq "" } {
     catch {common::send_msg_id "BD_TCL-106" "ERROR" "Unable to referenced block <$block_name>. Please add the files for ${block_name}'s definition into the project."}
     return 1
   }
  
  # Create instance: T74LS04_0, and set properties
  set block_name T74LS04
  set block_cell_name T74LS04_0
  if { [catch {set T74LS04_0 [create_bd_cell -type module -reference $block_name $block_cell_name] } errmsg] } {
     catch {common::send_msg_id "BD_TCL-105" "ERROR" "Unable to add referenced block <$block_name>. Please add the files for ${block_name}'s definition into the project."}
     return 1
   } elseif { $T74LS04_0 eq "" } {
     catch {common::send_msg_id "BD_TCL-106" "ERROR" "Unable to referenced block <$block_name>. Please add the files for ${block_name}'s definition into the project."}
     return 1
   }
  
  # Create instance: T74LS373_0, and set properties
  set block_name T74LS373
  set block_cell_name T74LS373_0
  if { [catch {set T74LS373_0 [create_bd_cell -type module -reference $block_name $block_cell_name] } errmsg] } {
     catch {common::send_msg_id "BD_TCL-105" "ERROR" "Unable to add referenced block <$block_name>. Please add the files for ${block_name}'s definition into the project."}
     return 1
   } elseif { $T74LS373_0 eq "" } {
     catch {common::send_msg_id "BD_TCL-106" "ERROR" "Unable to referenced block <$block_name>. Please add the files for ${block_name}'s definition into the project."}
     return 1
   }
  
  # Create instance: chip_select_0, and set properties
  set block_name chip_select
  set block_cell_name chip_select_0
  if { [catch {set chip_select_0 [create_bd_cell -type module -reference $block_name $block_cell_name] } errmsg] } {
     catch {common::send_msg_id "BD_TCL-105" "ERROR" "Unable to add referenced block <$block_name>. Please add the files for ${block_name}'s definition into the project."}
     return 1
   } elseif { $chip_select_0 eq "" } {
     catch {common::send_msg_id "BD_TCL-106" "ERROR" "Unable to referenced block <$block_name>. Please add the files for ${block_name}'s definition into the project."}
     return 1
   }
  
  # Create instance: m62256_0, and set properties
  set block_name m62256
  set block_cell_name m62256_0
  if { [catch {set m62256_0 [create_bd_cell -type module -reference $block_name $block_cell_name] } errmsg] } {
     catch {common::send_msg_id "BD_TCL-105" "ERROR" "Unable to add referenced block <$block_name>. Please add the files for ${block_name}'s definition into the project."}
     return 1
   } elseif { $m62256_0 eq "" } {
     catch {common::send_msg_id "BD_TCL-106" "ERROR" "Unable to referenced block <$block_name>. Please add the files for ${block_name}'s definition into the project."}
     return 1
   }
  
  # Create instance: m62256_1, and set properties
  set block_name m62256
  set block_cell_name m62256_1
  if { [catch {set m62256_1 [create_bd_cell -type module -reference $block_name $block_cell_name] } errmsg] } {
     catch {common::send_msg_id "BD_TCL-105" "ERROR" "Unable to add referenced block <$block_name>. Please add the files for ${block_name}'s definition into the project."}
     return 1
   } elseif { $m62256_1 eq "" } {
     catch {common::send_msg_id "BD_TCL-106" "ERROR" "Unable to referenced block <$block_name>. Please add the files for ${block_name}'s definition into the project."}
     return 1
   }
  
  # Create instance: splitter3_0, and set properties
  set block_name splitter3
  set block_cell_name splitter3_0
  if { [catch {set splitter3_0 [create_bd_cell -type module -reference $block_name $block_cell_name] } errmsg] } {
     catch {common::send_msg_id "BD_TCL-105" "ERROR" "Unable to add referenced block <$block_name>. Please add the files for ${block_name}'s definition into the project."}
     return 1
   } elseif { $splitter3_0 eq "" } {
     catch {common::send_msg_id "BD_TCL-106" "ERROR" "Unable to referenced block <$block_name>. Please add the files for ${block_name}'s definition into the project."}
     return 1
   }
  
  # Create instance: splitter_1_0, and set properties
  set block_name splitter_1
  set block_cell_name splitter_1_0
  if { [catch {set splitter_1_0 [create_bd_cell -type module -reference $block_name $block_cell_name] } errmsg] } {
     catch {common::send_msg_id "BD_TCL-105" "ERROR" "Unable to add referenced block <$block_name>. Please add the files for ${block_name}'s definition into the project."}
     return 1
   } elseif { $splitter_1_0 eq "" } {
     catch {common::send_msg_id "BD_TCL-106" "ERROR" "Unable to referenced block <$block_name>. Please add the files for ${block_name}'s definition into the project."}
     return 1
   }
  
  # Create instance: splitter_2_0, and set properties
  set block_name splitter_2
  set block_cell_name splitter_2_0
  if { [catch {set splitter_2_0 [create_bd_cell -type module -reference $block_name $block_cell_name] } errmsg] } {
     catch {common::send_msg_id "BD_TCL-105" "ERROR" "Unable to add referenced block <$block_name>. Please add the files for ${block_name}'s definition into the project."}
     return 1
   } elseif { $splitter_2_0 eq "" } {
     catch {common::send_msg_id "BD_TCL-106" "ERROR" "Unable to referenced block <$block_name>. Please add the files for ${block_name}'s definition into the project."}
     return 1
   }
  
  # Create port connections
  connect_bd_net -net Net [get_bd_pins PIC16F873_0/portb] [get_bd_pins T74LS373_0/dat] [get_bd_pins m62256_0/datw] [get_bd_pins m62256_1/datw]
  connect_bd_net -net Net1 [get_bd_pins T74LS04_0/inn] [get_bd_pins m62256_1/ce] [get_bd_pins m62256_1/we] [get_bd_pins splitter_1_0/d_out]
  connect_bd_net -net Net2 [get_bd_pins chip_select_0/oe] [get_bd_pins m62256_0/oe] [get_bd_pins m62256_1/oe] [get_bd_pins splitter3_0/d_out]
  connect_bd_net -net Net3 [get_bd_pins m62256_0/adr] [get_bd_pins m62256_1/adr] [get_bd_pins splitter_1_0/d_out2]
  connect_bd_net -net PIC16F873_0_porta [get_bd_pins PIC16F873_0/porta] [get_bd_pins splitter3_0/d_in] [get_bd_pins splitter_2_0/d_in]
  connect_bd_net -net PIC16F873_0_portc [get_bd_pins PIC16F873_0/portc] [get_bd_pins splitter_1_0/d_in]
  connect_bd_net -net T74LS04_0_outt [get_bd_pins T74LS04_0/outt] [get_bd_pins chip_select_0/not_in] [get_bd_pins m62256_0/ce] [get_bd_pins m62256_0/we]
  connect_bd_net -net T74LS373_0_outh [get_bd_pins T74LS373_0/outh] [get_bd_pins splitter_1_0/out_in]
  connect_bd_net -net chip_select_0_d_out [get_bd_pins PIC16F873_0/portbw] [get_bd_pins chip_select_0/d_out]
  connect_bd_net -net m62256_0_dat [get_bd_pins chip_select_0/d_in1] [get_bd_pins m62256_0/dat]
  connect_bd_net -net m62256_1_dat [get_bd_pins chip_select_0/d_in2] [get_bd_pins m62256_1/dat]
  connect_bd_net -net oe_0_1 [get_bd_ports oe_0] [get_bd_pins T74LS373_0/oe]
  connect_bd_net -net splitter_2_0_d_out [get_bd_pins T74LS373_0/le] [get_bd_pins splitter_2_0/d_out]

  # Create address segments


  # Restore current instance
  current_bd_instance $oldCurInst

  validate_bd_design
  save_bd_design
}
